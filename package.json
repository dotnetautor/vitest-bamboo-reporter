{
  "name": "vitest-bamboo-reporter",
  "version": "0.1.0",
  "type": "module",
  "description": "Bamboo reporter for vitest",
  "author": "Matthias Fischer <opensource@dotnetautor.de> (https://dotnetautor.de)",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vitest",
    "vitest-reporter",
    "bamboo",
    "reporter"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish": "clean-publish --package-manager pnpm --fields pnpm -- --no-git-checks",
    "publish:dev": "clean-publish --without-publish --package-manager pnpm --fields pnpm -- --no-git-checks"
  },
  "main": "dist/bamboo-reporter.umd.cjs",
  "module": "dist/bamboo-reporter.js",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@types/node": "^18.11.18",
    "clean-publish": "^4.1.1",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.27.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["eslint-config-airbnb"]
    },
    "packageExtensions": {
      "eslint-config-airbnb-typescript": {
        "peerDependencies": {
          "@typescript-eslint/eslint-plugin": "*"
        }
      }
    }
  }
}